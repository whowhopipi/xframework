<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<groupId>com.xframework.boot</groupId>
	<artifactId>xframework-boot-parent</artifactId>
	<version>0.2.2-SNAPSHOT</version>
	<packaging>pom</packaging>

	<name>xframework-boot-parent</name>
	<description>xframework-boot</description>

	<parent>
		<groupId>org.springframework.boot</groupId>
		<artifactId>spring-boot-starter-parent</artifactId>
		<version>1.5.2.RELEASE</version>
	</parent>

	<modules>
		<module>exception</module>
		<module>jdbc</module>
		<module>kaptcha</module>
		<module>security</module>
		<module>stat</module>
		<module>test</module>
		<module>web</module>
		<module>poi</module>
		<module>mybatis</module>
	</modules>

	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
		<java.version>1.6</java.version>
		<xframework-boot.version>0.0.1-SNAPSHOT</xframework-boot.version>
		<kaptcha.version>0.0.9</kaptcha.version>
		<lombok.version>1.16.12</lombok.version>
		<poi.version>3.15</poi.version>
		<commons-collections4.version>4.1</commons-collections4.version>
		<mybator.version>1.3.5</mybator.version>
		<tk.mapper.version>3.3.9</tk.mapper.version>
		<mybatis.version>3.4.2</mybatis.version>
		<mybatis.boot.version>1.2.0</mybatis.boot.version>
		<mybatis.pagehelper.version>1.0.0</mybatis.pagehelper.version>
		<tk.mapper.version>1.0.0</tk.mapper.version>
		<nekohtml.version>1.9.22</nekohtml.version>
		<webjars.locator.version>0.32</webjars.locator.version>
		<webjars.jquery>1.11.3</webjars.jquery>
		<webjars.bootstrap>3.3.7-1</webjars.bootstrap>
		<webjars.bootstrap-table>1.11.0</webjars.bootstrap-table>
		<webjars.bootstrap-tour>0.11.0</webjars.bootstrap-tour>
		<webjars.bootstrap-validator>0.11.5</webjars.bootstrap-validator>
		<webjars.chosen>1.6.2</webjars.chosen>
		<webjars.jquery-colorbox>1.6.4</webjars.jquery-colorbox>
		<webjars.flot>0.8.3-1</webjars.flot>
		<webjars.fullcalendar>2.8.0</webjars.fullcalendar>
	</properties>

	<dependencies>
		<dependency>
			<groupId>org.projectlombok</groupId>
			<artifactId>lombok</artifactId>
		</dependency>
	</dependencies>

	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>org.webjars</groupId>
				<artifactId>fullcalendar</artifactId>
				<version>${webjars.fullcalendar}</version>
			</dependency>

			<dependency>
				<groupId>org.webjars</groupId>
				<artifactId>flot</artifactId>
				<version>${webjars.flot}</version>
			</dependency>
			<dependency>
				<groupId>org.webjars</groupId>
				<artifactId>jquery-colorbox</artifactId>
				<version>${webjars.jquery-colorbox}</version>
			</dependency>
			<dependency>
				<groupId>org.webjars.bower</groupId>
				<artifactId>chosen</artifactId>
				<version>${webjars.chosen}</version>
			</dependency>

			<dependency>
				<groupId>org.webjars.bower</groupId>
				<artifactId>bootstrap-validator</artifactId>
				<version>${webjars.bootstrap-validator}</version>
			</dependency>

			<dependency>
				<groupId>org.webjars</groupId>
				<artifactId>bootstrap-tour</artifactId>
				<version>${webjars.bootstrap-tour}</version>
			</dependency>

			<dependency>
				<groupId>org.webjars.bower</groupId>
				<artifactId>bootstrap-table</artifactId>
				<version>${webjars.bootstrap-table}</version>
			</dependency>

			<dependency>
				<groupId>org.webjars</groupId>
				<artifactId>bootstrap</artifactId>
				<version>${webjars.bootstrap}</version>
			</dependency>

			<dependency>
				<groupId>org.webjars</groupId>
				<artifactId>jquery</artifactId>
				<version>${webjars.jquery}</version>
			</dependency>

			<dependency>
				<groupId>org.webjars</groupId>
				<artifactId>webjars-locator</artifactId>
				<version>${webjars.locator.version}</version>
			</dependency>

			<dependency>
				<groupId>net.sourceforge.nekohtml</groupId>
				<artifactId>nekohtml</artifactId>
				<version>${nekohtml.version}</version>
			</dependency>

			<dependency>
				<groupId>org.mybatis.spring.boot</groupId>
				<artifactId>mybatis-spring-boot-starter</artifactId>
				<version>${mybatis.boot.version}</version>
				<type>jar</type>
			</dependency>
			<dependency>
				<groupId>tk.mybatis</groupId>
				<artifactId>mapper-spring-boot-starter</artifactId>
				<version>${tk.mapper.version}</version>
				<type>jar</type>
			</dependency>
			<dependency>
				<groupId>com.github.pagehelper</groupId>
				<artifactId>pagehelper-spring-boot-starter</artifactId>
				<version>${mybatis.pagehelper.version}</version>
				<type>jar</type>
			</dependency>

			<dependency>
				<groupId>org.projectlombok</groupId>
				<artifactId>lombok</artifactId>
				<version>${lombok.version}</version>
			</dependency>
			<dependency>
				<groupId>com.xframework.boot</groupId>
				<artifactId>xframework-boot-web</artifactId>
				<version>${xframework-boot.version}</version>
				<type>jar</type>
			</dependency>
			<dependency>
				<groupId>com.xframework.boot</groupId>
				<artifactId>xframework-boot-exception</artifactId>
				<version>${xframework-boot.version}</version>
				<type>jar</type>
			</dependency>
			<dependency>
				<groupId>com.xframework.boot</groupId>
				<artifactId>xframework-boot-kaptcha</artifactId>
				<version>${xframework-boot.version}</version>
				<type>jar</type>
			</dependency>
			<dependency>
				<groupId>com.xframework.boot</groupId>
				<artifactId>xframework-boot-jdbc</artifactId>
				<version>${xframework-boot.version}</version>
				<type>jar</type>
			</dependency>
			<dependency>
				<groupId>com.xframework.boot</groupId>
				<artifactId>xframework-boot-poi</artifactId>
				<version>${xframework-boot.version}</version>
				<type>jar</type>
			</dependency>
			<dependency>
				<groupId>com.xframework.boot</groupId>
				<artifactId>xframework-boot-mybatis</artifactId>
				<version>${xframework-boot.version}</version>
				<type>jar</type>
			</dependency>
			<dependency>
				<groupId>com.github.axet</groupId>
				<artifactId>kaptcha</artifactId>
				<version>${kaptcha.version}</version>
			</dependency>
			<dependency>
				<groupId>org.apache.poi</groupId>
				<artifactId>poi</artifactId>
				<version>${poi.version}</version>
				<type>jar</type>
			</dependency>
			<dependency>
				<groupId>org.apache.poi</groupId>
				<artifactId>poi-scratchpad</artifactId>
				<version>${poi.version}</version>
				<type>jar</type>
			</dependency>
			<dependency>
				<groupId>org.apache.poi</groupId>
				<artifactId>poi-ooxml</artifactId>
				<version>${poi.version}</version>
				<type>jar</type>
			</dependency>
			<dependency>
				<groupId>org.apache.poi</groupId>
				<artifactId>poi-ooxml-schemas</artifactId>
				<version>${poi.version}</version>
				<type>jar</type>
			</dependency>
			<dependency>
				<groupId>org.apache.poi</groupId>
				<artifactId>poi-examples</artifactId>
				<version>${poi.version}</version>
				<type>jar</type>
			</dependency>
			<dependency>
				<groupId>org.apache.poi</groupId>
				<artifactId>poi-ooxml-schemas</artifactId>
				<version>${poi.version}</version>
				<type>jar</type>
			</dependency>
			<dependency>
				<groupId>org.apache.commons</groupId>
				<artifactId>commons-collections4</artifactId>
				<version>${commons-collections4.version}</version>
				<type>jar</type>
			</dependency>

			<dependency>
				<groupId>org.mybatis.generator</groupId>
				<artifactId>mybatis-generator-core</artifactId>
				<version>${mybator.version}</version>
				<type>jar</type>
			</dependency>

			<dependency>
				<groupId>org.mybatis</groupId>
				<artifactId>mybatis</artifactId>
				<version>${mybatis.version}</version>
			</dependency>

		</dependencies>
	</dependencyManagement>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-source-plugin</artifactId>
				<configuration>
					<attach>true</attach>
				</configuration>
				<executions>
					<execution>
						<phase>compile</phase>
						<goals>
							<goal>jar</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
</project>
